<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\icons8-image-40.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icons8-cancel-48.png" />
    <None Remove="icons8-clear-48.png" />
    <None Remove="icons8-convert-48.png" />
    <None Remove="icons8-image-40.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\icons8-image-40.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="8.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons8-clear-48.png" />
    <Resource Include="Resources\icons8-convert-48.png" />
    <Resource Include="Resources\icons8-image-40.png" />
  </ItemGroup>

</Project>
